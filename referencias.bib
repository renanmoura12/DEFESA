@article{Souza_Prestes_Santos_Cruz_2025, title={A IMPORTÂNCIA DO PRÉ-NATAL PARA SAÚDE MATERNO-INFANTIL}, volume={18}, url={https://ojs.focopublicacoes.com.br/foco/article/view/10833}, DOI={10.54751/revistafoco.v18n12-009}, abstractNote={&amp;lt;p&amp;gt;Introdução A atenção pré-natal é reconhecida como uma das estratégias mais eficazes para a redução da morbimortalidade materna e neonatal, sendo o enfermeiro um protagonista nesse processo por atuar tanto na prevenção quanto na detecção precoce de riscos gestacionais. Objetivo: analisar a atuação do enfermeiro como agente de prevenção e detecção precoce de riscos durante o pré-natal, destacando sua relevância para a promoção da saúde materno-fetal. Método: Trata-se de uma pesquisa integrativa de literatura, fundamentada em artigos publicados entre 2020 a 2025 nas bases LILACS, BDENF, PubMed/MEDLINE e BVS. Resultados: Foram encontrados &amp;amp;nbsp;150 estudos nas bases de dados, mas somente 10 foram incluídos na amostra final. Conclusão: A atuação do enfermeiro no pré-natal mostra-se essencial para a promoção da saúde materno-fetal. Por meio do acompanhamento clínico, solicitação de exames e ações educativas, o profissional contribui para a prevenção e detecção precoce de riscos gestacionais. Sua intervenção favorece a adesão das gestantes ao acompanhamento e reduz complicações maternas e neonatais.&amp;lt;/p&amp;gt;}, number={12}, journal={REVISTA FOCO}, author={Souza, Geovana Leal de and Prestes, Karine Matos and Santos, Rayssa Sousa dos and Cruz, Ann Caroline Nascimento}, year={2025}, month=dez, pages={e10833} }

